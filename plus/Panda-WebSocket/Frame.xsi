MODULE = Panda::WebSocket                PACKAGE = Panda::WebSocket::Frame
PROTOTYPES: DISABLE

BOOT {
    xs::register_package("Panda::WebSocket::Frame", "Panda::WebSocket");
}

string error (panda::websocket::FrameSP THIS) {
    if (!THIS->error) XSRETURN_UNDEF;
    RETVAL = THIS->error;
}

int opcode (panda::websocket::FrameSP THIS) {
    RETVAL = THIS->opcode();
}

bool is_control (panda::websocket::FrameSP THIS) {
    RETVAL = THIS->is_control();
}

bool final (panda::websocket::FrameSP THIS) {
    RETVAL = THIS->final();
}

size_t payload_length (panda::websocket::FrameSP THIS) {
    RETVAL = THIS->payload_length();
}

SV* payload (panda::websocket::FrameSP THIS) {
    RETVAL = vector_string_to_sv(THIS->payload);
}

uint16_t close_code (panda::websocket::FrameSP THIS) {
    RETVAL = THIS->close_code();
}

string close_message (panda::websocket::FrameSP THIS) {
    RETVAL = THIS->close_message();
}

void DESTROY (panda::websocket::FrameSP THIS) {}
